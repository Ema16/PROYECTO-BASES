                                        <option value="" disabled="" selected="">Selecciona su carrera...</option>
                                    </select>
                                    <span asp-validation-for="CodCarrera" class="text-danger"></span>
                                </div>



                                <div class="form-group" style="padding-left:20%; padding-right:20%">
                                    <input type="submit" name="submit" id="submit" class="form-submit" value="Registrarse" />
                                </div>
                            </form>
